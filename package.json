{
  "name": "tanks",
  "version": "0.1.0",
  "description": "Tanks game has written in JavaScript",
  "main": "src/index.js",
  "scripts": {
    "assets:watch": "rm -rf dist/ && supervisor -w server/assets-server.js server/assets-server.js",
    "assets:build": "rm -rf dist/ && webpack -p",
    "server": "supervisor -w server/ server/server.js",
    "start": "concurrently --raw \"npm run assets:watch\" \"npm run server\"",
    "cover": "jest --coverage",
    "tdd": "npm test -- --watch",
    "pretest": "eslint src/ server/",
    "test": "NODE_ENV=test jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Nitive/tanks.git"
  },
  "keywords": [
    "tanks",
    "game"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Nitive/tanks/issues"
  },
  "homepage": "https://github.com/Nitive/tanks#readme",
  "devDependencies": {
    "babel-core": "^6.18.0",
    "babel-eslint": "^7.1.0",
    "babel-jest": "^16.0.0",
    "babel-loader": "^6.2.7",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.18.0",
    "babel-plugin-transform-flow-strip-types": "^6.18.0",
    "babel-plugin-transform-object-rest-spread": "^6.16.0",
    "babel-preset-latest": "^6.16.0",
    "codeclimate-test-reporter": "^0.4.0",
    "concurrently": "^3.1.0",
    "eslint": "^3.8.1",
    "eslint-config-as": "^0.1.6",
    "express": "^4.14.0",
    "file-loader": "^0.9.0",
    "flow-bin": "^0.33.0",
    "flow-typed": "^2.0.0",
    "html-webpack-plugin": "^2.24.0",
    "jest": "^16.0.2",
    "morgan": "^1.7.0",
    "supervisor": "^0.11.0",
    "webpack": "2.1.0-beta.25",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-hot-middleware": "^2.13.1"
  },
  "dependencies": {
    "@cycle/dom": "^13.0.0",
    "@cycle/xstream-run": "^3.1.0",
    "cycle-keyboard": "1.0.0-rtm.4",
    "express-http-proxy": "^0.10.1",
    "ramda": "^0.22.1",
    "xstream": "^7.0.0"
  }
}
